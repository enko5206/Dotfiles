! ------------------------------------------------------------------------------
! file: 				             ~/.Xdefaults
! author: 			             	 enko
! last updated:      				 09.06.2010
! ------------------------------------------------------------------------------
!
! X F T S E T T I N G S
!
Xft.dpi:                 			 96.0
Xft.hinting:             			 true
Xft.hintstyle:           			 hintslight
Xft.antialias:           			 true
Xft.rgba:                			 none
! ------------------------------------------------------------------------------
!
! X C U R S O R
!
Xcursor.theme:           			 Vanilla-DMZ-AA
Xcursor.size:            			 22
! ------------------------------------------------------------------------------
!
! F O N T S E T T I N G S
!
urxvt*font:              			 xft:Consolas:pixelsize=12:antialias=true:hinting=true
!urxvt*font:              			 xft:Terminus:pixelsize=12:antialias=true:hinting=true
!urxvt*font:              			 xft:Monaco:pixelsize=10:antialias=true:hinting=true
!
! ------------------------------------------------------------------------------
!
! U R X V T
!
urxvt.geometry:          			 80x23
urxvt.depth:             			 32
urxvt.loginShell:        			 true
urxvt.scrollBar:         			 false
urxvt.jumpScroll:        			 true
urxvt.skipScroll:        			 false
urxvt.secondaryScroll:   			 false
urxvt.buffered:          			 true
urxvt.saveLines:         			 65535
urxvt.cursorBlink:       			 true
urxvt.urgentOnBell:      			 true
urxvt.override-redirect: 			 false
urxvt.borderLess:        			 false
urxvt.shading:           			 30
urxvt.intensityStyles:   			 true
urxvt.transparent:       			 false
!--
urxvt.perl-ext-common:   			 default,matcher,tabbed
urxvt.perl-lib:          			 /usr/lib/urxvt/perl/
urxvt.urlLauncher:       			 firefox
urxvt.matcher.button:    	                 1
! --
urxvt.scrollstyle:       			 xterm
urxvt.tabbed.tabbar-fg:  	                 6
urxvt.tabbed.tabbar-bg:  			 0
urxvt.tabbed.tab-fg:     			 6
urxvt.tabbed.tab-bg:     			 0
!
! ------------------------------------------------------------------------------
!
! C O L O R S E T T I N G S
!
!urxvt*foreground:       			 #dcdccc
!urxvt*background:       			 rgba:0000/0000/0000/dddd
!
urxvt.tintColor:           			 white
urxvt.cursorColor:       			 #dd9900
urxvt.cursorColor2:      			 #000000
urxvt.colorBD:           			 #ffffff
urxvt.colorIT:           			 #bbbbbb
urxvt.colorUL:           			 #999999
urxvt.underlineColor:    			 #999999
